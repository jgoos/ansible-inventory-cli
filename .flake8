[flake8]
max-line-length = 88
max-complexity = 10
extend-ignore =
    E203,
    E501,
    W503,
    W504,
    C901,
exclude =
    .git,
    __pycache__,
    .tox,
    .eggs,
    *.egg,
    build,
    dist,
    .venv,
    venv,
    env,
    ENV,
    .pytest_cache,
    .mypy_cache,
    logs,
    backups,
    archive
per-file-ignores =
    __init__.py:F401
    scripts/config.py:E501
docstring-convention = google
import-order-style = google 